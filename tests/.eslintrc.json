{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "project": "./tsconfig.json"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "ignorePatterns": ["*.js", "*.mjs", "**/tests/helpers/**"],
    "rules": {
        "no-empty-function": "off",
        "@typescript-eslint/no-empty-function": ["off"]
    }
}
